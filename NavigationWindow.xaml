<Window x:Class="WpfApp1.NavigationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Navigation Window" Height="350" Width="525">
    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <Button Content="Window 1" Click="Button_Click_1"/>
            <Button Content="Window 2" Click="Button_Click_2"/>
            <Button Content="Window 3" Click="Button_Click_3"/>
            <Button Content="Window 4" Click="Button_Click_4"/>
            <Button Content="Window 5" Click="Button_Click_5"/>
            <Button Content="Window 6" Click="Button_Click_6"/>
        </StackPanel>
        <Frame Name="MainFrame"/>
    </DockPanel>
</Window>
